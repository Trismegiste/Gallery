var cat{{ idx }} = PhotoSwipe.attach(
        [
            {% for photo in listing %}
            {url: '{{ photo.url }}', caption: '{{ photo.caption }}'}
            {% if not loop.last %},{% endif %}
            {% endfor %}
        ],
        options
        );

document.getElementById('cat{{ idx }}').addEventListener('click', function() {
    cat{{ idx }}.show(0);
});
